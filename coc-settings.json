{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "html",
    "javascript",
    "javascriptreact",
    "json",
    "graphql",
    "markdown",
    "typescript",
    "typescriptreact",
    "yaml"
  ],
  "diagnostic.errorSign": "‚ò†Ô∏è'",
  "diagnostic.hintSign": "üí°",
  "diagnostic.infoSign": "‚ÑπÔ∏è",
  "diagnostic.warningSign": "‚ö†Ô∏è'",
  "eslint.autoFixOnSave": true,
  "list.indicator": "üëΩ",
  "list.insertMappings": {
    "<C-t>": "action:tabe",
    "<C-v>": "action:vsplit",
    "<C-x>": "action:split",
    "<C-z>": "do:toggle"
  },
  "prettier.disableSuccessMessage": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "suggest.enablePreview": true,
  "typescript.updateImportsOnFileMove.enable": false,
  "languageserver": {
    "graphql": {
      "command": "graphql-lsp",
      "args": ["server", "-m", "stream"],
      "filetypes": ["typescript", "typescriptreact", "graphql"]
    },
    "tailwind-lsp": {
      "command": "tailwindcss-language-server",
      "args": ["--stdio"],
      "filetypes": [
        "css",
        "html",
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
      ],
      "rootPatterns": [
        "tailwind.config.js",
        "package.json",
        "node_modules",
        ".git"
      ],
      "settings": {
        "tailwindCSS": {
          "classAttributes": ["class", "className", "classNameList"]
        }
      }
    }
  }
}
